{
    "name": "My Scheduled Open Url",
    "manifest_version": 2,
    "short_name": "MySOU",
    "author": "mao-miyaji",
    "version": "1.0.0",
    "description": "指定日時に対象Urlを開くスケジューラー。ChromeのLocalStorageを使用します。",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {
        "default_popup": "index.html#/list"
    },
    "background": {
        "page": "index.html#/event",
        "persistent": true
    },
    "permissions": [
        "tabs"
    ]
}