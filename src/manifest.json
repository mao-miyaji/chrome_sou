{
    "name": "My Scheduled Open Url",
    "manifest_version": 2,
    "short_name": "MySOU",
    "version": "1.0.0",
    "description": "指定日時に対象Urlを開くスケジューラー",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "bookmarks"
    ]
}